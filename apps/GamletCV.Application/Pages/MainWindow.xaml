<Window x:Class="GamletCV.Application.Pages.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GamletCV"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Left">
        <ComboBox Name="ComboBoxCameraName" SelectionChanged="ComboBoxCameraNameSelectionChanged"></ComboBox>
        <Button Click="ButtonStartCamera">Start</Button>
        <Button Click="ButtonStopCamera">Stop</Button>
    </StackPanel>
</Window>
